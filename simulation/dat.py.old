#!/usr/bin/env python

import sys
import json

import sim

def extract(sample, field):
    values = [s[field] for s in sample]
    values.sort()
    return sim.median(values)

raw = json.load(sys.stdin)[sys.argv[1]]
dat = dict()

for k, v in raw.items():
    dat[float(k)] = v

fields = dat.values()[0][0].keys()
fields.sort()
print(' '.join(['alpha'] + fields))

alphas = dat.keys()
alphas.sort()

for i in alphas:
    print(' '.join([str(i)] + [str(extract(dat[i], f)) for f in fields]))
